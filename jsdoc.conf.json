{
    "plugins": ["plugins/markdown"],
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "opts": {
        "template": "tui-jsdoc-template",
        "tutorials": "examples",
        "package": "package.json",
        "recurse": true
    },
    "templates": {
        "name": "RPScript Documentation",
        "footerText":"Typecasting SG",
        "useCollapsibles": true,
        "tabNames": {
            "api": "API",
            "tutorials": "Examples"
        }
    }
}