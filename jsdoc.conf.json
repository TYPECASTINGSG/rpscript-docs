{
    "plugins": ["plugins/markdown"],
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "opts": {
        "template": "tui-jsdoc-template",
        "tutorials": "articles",
        "package": "package.json",
        "recurse": true
    },
    "templates": {
        "name": "RPScript",
        "footerText":"RPScript",
        "useCollapsibles": true,
        "tabNames": {
            "api": "API",
            "tutorials": "Documentation"
        },
        "default": {
            "staticFiles": {
                "include": ["static/"]
            }
        },
        "css": [
            "styles/custom.css"
        ]
    }
}